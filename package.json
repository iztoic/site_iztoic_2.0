  },
  "dependencies": {
    "canvas": "^2.11.2",
    "jquery": "npm:jQuery@^1.7.4",
    "jQuery": "^1.7.4",
    "jsdom": "^23.0.1",
    "location": "^0.0.1",
    "navigator": "^1.0.1",
    "vue": "^2.0.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
